{
  "name": "cognitive-services-test-app",
  "version": "0.0.2",
  "main": "public/electron.js",
  "homepage": "./",
  "private": true,
  "engines": {
    "npm": ">=7.24.0",
    "node": ">=16.10.0"
  },
  "scripts": {
    "start": "yarn start:electron",
    "start:web": "env-cmd -f .env.web craco start",
    "start:electron": "electron .",
    "start:electron-dev": "electron electron-dev.js",
    "build": "yarn build:electron",
    "build:web": "craco build",
    "build:electron": "env-cmd -f .env.electron craco build",
    "watch:electron": "npm-watch",
    "test": "craco test",
    "eject": "react-scripts eject",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "rebuild": "electron-rebuild -f -w grpc"
  },
  "watch": {
    "build:electron": "src/**/*.*"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "axios": "^0.27.2",
    "cognitiveserviceslib": "github:wwlib/cognitiveserviceslib#v0.1.3",
    "electron": "^5.0.0",
    "jsonfile": "^6.1.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "robokit-command-system": "github:wwlib/robokit-command-system#v0.0.1",
    "socket.io-client": "^4.5.1",
    "timesync": "^1.0.10",
    "wavefile": "^11.0.0",
    "ww-music": "github:wwlib/ww-music#feature/andrew/update-modules"
  },
  "devDependencies": {
    "@craco/craco": "^5.9.0",
    "@types/node": "^18.0.0",
    "@types/react": "^16.14.28",
    "@types/react-dom": "^16.9.16",
    "electron-builder": "^23.1.0",
    "electron-rebuild": "^3.2.7",
    "env-cmd": "^10.1.0",
    "npm-watch": "^0.11.0",
    "react-scripts": "^3.4.4",
    "typescript": "^3.6.4",
    "webpack-node-externals": "^3.0.0"
  },
  "build": {
    "asar": false,
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "package.json",
      "audio/**/*",
      "protos/**/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:wwlib/cognitive-services-test-app.git"
  },
  "keywords": [
    "react,create-react-app,cra,craco,electron,electron-builder"
  ],
  "author": "Andrew Rapo",
  "license": "MIT"
}
