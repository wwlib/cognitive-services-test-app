{
  "name": "cognitive-services-test-app",
  "version": "0.0.1",
  "main": "public/electron.js",
  "homepage": "./",
  "private": true,
  "scripts": {
    "start": "yarn start:electron",
    "start:web": "env-cmd -f .env.web craco start",
    "start:electron": "electron .",
    "start:electron-dev": "electron electron-dev.js",
    "build": "yarn build:electron",
    "build:web": "craco build",
    "build:electron": "env-cmd -f .env.electron craco build",
    "watch:electron": "npm-watch",
    "test": "craco test",
    "eject": "react-scripts eject",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "rebuild": "electron-rebuild -f -w grpc"
  },
  "watch": {
    "build:electron": "src/**/*.*"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies-grpc": {
    "@grpc/proto-loader": "^0.5.3",
    "grpc": "^1.24.2",
    "protobufjs": "^6.8.8"
  },
  "dependencies": {
    "cognitiveserviceslib": "wwlib/cognitiveserviceslib#v0.0.6",
    "jsonfile": "^6.0.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "wavefile": "^11.0.0",
    "ww-music": "^0.0.6"
  },
  "devDependencies": {
    "@craco/craco": "^5.6.1",
    "@types/jest": "24.0.21",
    "@types/node": "12.12.3",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.3",
    "electron": "^5.0.0",
    "electron-builder": "^22.1.0",
    "electron-rebuild": "^1.8.8",
    "env-cmd": "^10.0.1",
    "npm-watch": "^0.6.0",
    "react-scripts": "^3.3.0",
    "typescript": "3.6.4",
    "webpack-node-externals": "^1.7.2"
  },
  "build": {
    "asar": false,
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "package.json",
      "audio/**/*",
      "protos/**/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:wwlib/cognitive-services-test-app.git"
  },
  "keywords": [
    "react,create-react-app,cra,craco,electron,electron-builder"
  ],
  "author": "Andrew Rapo",
  "license": "MIT"
}
